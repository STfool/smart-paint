{
  "name": "smart-paint",
  "version": "1.0.0",
  "private": true,
  "repository": {
    "type": "github",
    "url": "https://github.com/STfool/smart-paint"
  },
  "main": "app/main",
  "description": "awesome instant painting application running in any platform",
  "scripts": {
    "dev": "webpack-dev-server --config webpack/webpack.dev.js",
    "build": "webpack --config webpack/webpack.prod.js",
    "deploy": "npm run build && deploy.sh",
    "test": "npm run build",
    "electron": "electron .",
    "start": "concurrently \" npm run dev \" \"wait-on http://localhost:3000/ && npm run electron\""
  },
  "keywords": [
    "electron",
    "react.js",
    "react-hooks"
  ],
  "author": "Tangjj1996",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "chalk": "^4.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.1",
    "electron": "^8.2.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "file-loader": "^6.0.0",
    "glob": "^7.1.6",
    "html-webpack-plugin": "^4.0.4",
    "husky": "^4.2.5",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "lint-staged": "^10.1.3",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss-loader": "^3.0.0",
    "style-loader": "^1.1.3",
    "svgo": "^1.3.2",
    "svgo-loader": "^2.2.1",
    "wait-on": "^4.0.2",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "node scripts/commit-lint.js"
    }
  },
  "lint-staged": {
    "**/*.js(x)?": "eslint --fix"
  },
  "browserslist": "> 0.25%, not dead"
}
